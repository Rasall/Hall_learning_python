programa {
  funcao inicio() {
    inteiro n6 
    inteiro n7 
    inteiro mult, som
    escreva("digite o primero numero: ")
    leia(n6)
    escreva("\n")
    escreva("digite o segundo numero: ")
    leia(n7)
    escreva("\n")
    mult = n6 * n7
    escreva("Multiplicação = ", mult)
    escreva("\n")

    som = (n6 / 2) + n7
    escreva("Soma =  ", som)
    escreva("\n")
  }
}
