programa {
  funcao inicio() {
    inteiro n1, n2, soma
    escreva("Digite o primeiro valor: ")
    leia(n1)
    escreva("Digite o segundo valor: ")
    leia(n2)
    soma=n1+n2
    se(soma == 50){
      escreva("Resultado é igual a 50")
    }senao{
      escreva("Resultado é diferente a 50")
    }
  }
}
