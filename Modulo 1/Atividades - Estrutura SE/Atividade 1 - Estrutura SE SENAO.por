programa {
  funcao inicio() {
    inteiro numero
    escreva("Olá digite um numero: " )
    leia(numero)
    se(numero > 0){
      escreva("O numero digitado é positivo")
    }senao se (numero < 0){
      escreva("O numero digitado  é negativo")
    }senão{
      escreva("O numero digitado  é 0")
    }
  }
}
