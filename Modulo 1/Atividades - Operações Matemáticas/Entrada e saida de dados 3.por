programa {
  funcao inicio() {
    inteiro dia,mes,ano
    escreva("Olá, Digite o mês que você nasceu: ")
    leia(mes)
    escreva("\n")
    escreva("Digite o dia que você nasceu: ")
    leia(dia)
    escreva("\n")
    escreva("Digite o do ano que você nasceu: ")
    leia(ano)
    escreva("\n")
    escreva("Você nasceu no dia "+dia+" do mês "+mes+" do ano "+ano)
  }
}
