programa {
inclua biblioteca Util --> util 
  funcao inicio() {
    inteiro numeros[4][3]
  para(inteiro i = 0; i < 3; i++){
  para(inteiro f = 0; f < 3; f++){
    escreva("Insira numero ALEATORIO: ")
    leia(numeros[i][f]) 
  }}
  
  para(inteiro g = 0; g < 3; g++){
  para(inteiro h = 0; h < 3; h++){
    escreva(numeros[g][h]," ") 
  }escreva("\n")
  }

}}